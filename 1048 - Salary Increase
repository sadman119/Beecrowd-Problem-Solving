#include <stdio.h>

int main() {
    float sala, new_salary;
    scanf("%f", &sala);

    if (sala >= 0 && sala <= 400) {
        new_salary = sala + (sala * 0.15);
        printf("Novo salario: %.2f\n", new_salary);
        printf("Reajuste ganho: %.2f\n", (sala * 0.15));
        printf("Em percentual: 15 %%\n");
    } else if (sala > 400 && sala <= 800) {
        new_salary = sala + (sala * 0.12);
        printf("Novo salario: %.2f\n", new_salary);
        printf("Reajuste ganho: %.2f\n", (sala * 0.12));
        printf("Em percentual: 12 %%\n");
    } else if (sala > 800 && sala <= 1200) {
        new_salary = sala + (sala * 0.10);
        printf("Novo salario: %.2f\n", new_salary);
        printf("Reajuste ganho: %.2f\n", (sala * 0.10));
        printf("Em percentual: 10 %%\n");
    } else if (sala > 1200 && sala <= 2000) {
        new_salary = sala + (sala * 0.07);
        printf("Novo salario: %.2f\n", new_salary);
        printf("Reajuste ganho: %.2f\n", (sala * 0.07));
        printf("Em percentual: 7 %%\n");
    } else {
        new_salary = sala + (sala * 0.04);
        printf("Novo salario: %.2f\n", new_salary);
        printf("Reajuste ganho: %.2f\n", (sala * 0.04));
        printf("Em percentual: 4 %%\n");
    }

    return 0;
}
