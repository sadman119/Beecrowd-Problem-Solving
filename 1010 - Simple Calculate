#include <stdio.h>
 
int main() {
 
    int code;
    float price, quantity;
    scanf("%d %f %f", &code, &quantity, &price);
    float total = quantity * price; 
    scanf("%d %f %f", &code, &quantity, &price);
    total = total+ quantity * price; 
    printf("VALOR A PAGAR: R$ %.2f\n", total);
    return 0;
}
