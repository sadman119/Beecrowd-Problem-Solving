#include <stdio.h>
 
int main() {
 
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    
    int a1,b1,c1;
    scanf("%d %d %d",&a1,&b1,&c1);
 
    int a2,b2,c2;
    
    if(a1>a)
    a2=a1-a;
    
    if(b1>b)
    b2=b1-b;
    
    if(c1>c)
    c2=c1-c;
    
    printf("%d\n",a2+b2+c2);
 
    return 0;
}
