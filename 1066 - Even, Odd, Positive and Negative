#include <stdio.h>
 
int main() {
    int num,counteven=0,countodd=0,countpos=0,countneg=0;
 
    for (int i=0; i<5; i++){
        scanf("%d",&num);
        if(num%2==0)
        counteven++;
        if(num%2==1)
        countodd++;
        if(num>0)
        countpos++;
        if(num<0)
        countneg++;
        
    }
    printf("%d valor(es) par(es)\n",counteven);
    printf("%d valor(es) impar(es)\n",countodd);
    printf("%d valor(es) positivo(s)\n",countpos);
    printf("%d valor(es) negativo(s)\n",countneg);
    return 0;
}
