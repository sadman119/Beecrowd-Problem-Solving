#include <stdio.h>
#include <string.h>

int main() {
    int cycle,newton;
    scanf("%d", &cycle);

    char name[20];

    for (int i = 0; i < cycle; i++) {
        scanf("%s %d", name, &newton); 
        
        if (strcmp(name, "Thor") == 0) {
            printf("Y\n");
        } else {
            printf("N\n");
        }
    }
    return 0;
}
