#include <stdio.h>

int main() {
    int round,x, y,c1=0,c2=0;
    scanf("%d", &round);
    while (round != 0) {
        c1=0;
        c2=0;
        for (int i = 0; i < round; i++) {
             scanf("%d %d", &x, &y);
            if (x > y) 
                c1++;
            else if (x < y) 
                c2++;       
        }
        printf("%d %d\n", c1, c2);
        scanf("%d", &round);
    }
    return 0;
}
