#include <stdio.h>
 
int main() {
 
    int start,end;
    scanf("%d %d",&start,&end);
    
   if(start>end){
        int dif= (24-start)+end;
        printf("O JOGO DUROU %d HORA(S)\n",dif);
   }
   else if(start==end)
   printf("O JOGO DUROU 24 HORA(S)\n");
   else{
       int dif2= (end-start);
       printf("O JOGO DUROU %d HORA(S)\n",dif2);
   }
 
    return 0;
}
